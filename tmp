# dealer master excel中的数据，如果要添加到lsh经销商表，字段要一个个查比较麻烦，在excel中用以下的公式，可以拼成一个insert into语句
=CONCATENATE("insert into lsh经销商表 values('",S87,"','",T87,"','",K87,"','",J87,"','","','","','",Z87,"','",E87,"','",AA87,"','",F87,"','",Y87,"','",D87,"','",AB87,"','",Q87,"','",R87,"','",AC87,"','",C87,"','",B87,"','",P87,"','",O87,"','",AF87,"',null,'",L87,"','",H87,"','",M87,"','",N87,"','",TEXT(I87,"yyyy-mm-dd"),"',null,'",U87,"','",V87,"','",W87,"','",AD87,"','",AE87,"','否","','Admin","',current_timestamp(),","'",X87,"')")

